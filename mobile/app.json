{
  "expo": {
    "name": "Oasis App",
    "slug": "oasis-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b141a"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      ["expo-notifications", { "icon": "./assets/icon.png", "color": "#ffffff" }],
      "expo-image-picker",
      "expo-av",
      "@zegocloud/zego-uikit-prebuilt-call-rn",
      "@zegocloud/zego-uikit-signaling-plugin-rn"
    ],
    "android": {
      "package": "com.oasis.app2026",
      "googleServicesFile": "./google-services.json",
      "usesCleartextTraffic": true, 
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "ACCESS_WIFI_STATE",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "metaData": {
        "com.startapp.sdk.APPLICATION_ID": "201405341"
      }
    },
    "extra": {
      "eas": {
        "projectId": "742d678c-90bd-4cf1-b261-604691b3cf53"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/742d678c-90bd-4cf1-b261-604691b3cf53"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
